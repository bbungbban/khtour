<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ksmart42.khtour.mapper.AccommodationMapper">

<!-- 리뷰등록 SQL -->
<insert id="AccomReview">
		INSERT INTO
		tb_ldg_review
		( ldg_review_code
		 ,member_id
		 ,ldg_code
		 ,ldg_cleanliness
		 ,ldg_kindness
		 ,ldg_convenience
		 ,ldg_location
		 ,ldg_price_sta
		 ,review_contents
		 ,review_time
		)VALUES (
		sf_ldg_review_code()
		,#{memberId}
		,#{ldgCode}
		,#{ldgCleanliness}
		,#{ldgKindness}
		,#{ldgConvenience}
		,#{ldgLocation}
		,#{ldgPriceSta}
		,#{reviewContents}
		,#{reviewTime}
		);
</insert>

</mapper>